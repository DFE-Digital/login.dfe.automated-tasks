{
  "name": "login.dfe.automated-tasks",
  "version": "3.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev:checks": "npm run lint && npm run test",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . -f junit -o eslint-results.xml",
    "prepare": "husky",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "jest --coverage",
    "test:report": "jest --coverage --reporters=default --reporters jest-junit --coverageReporters=clover",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@azure/functions": "^4.10.0",
    "@azure/msal-node": "^2.16.3",
    "@azure/service-bus": "^7.9.5",
    "login.dfe.jobs-client": "^6.1.3",
    "sequelize": "^6.37.7",
    "tedious": "^19.1.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.x",
    "@types/ws": "^8.5.13",
    "eslint": "^9.19.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^4.0.0",
    "typescript-eslint": "^8.21.0"
  },
  "main": "dist/src/index.js"
}
